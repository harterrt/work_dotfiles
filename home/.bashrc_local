alias scan='sudo iwlist wlp4s0 scan'
synclient VertEdgeScroll=0
synclient MaxTapTime=0

alias apip=$HOME/anaconda2/bin/pip
export SPARK_HOME=$HOME/bin/spark-2.0.0-bin-hadoop2.7

lpoptions -d MFCL2700DW > /dev/null

# For running tests that depend on spark
export PYTHONPATH=/home/harterrt/bin/spark-2.0.2-bin-hadoop2.7/python/
export SPARK_HOME=/home/harterrt/bin/spark-2.0.2-bin-hadoop2.7/

# added by Miniconda2 4.3.11 installer
export PATH="/home/harterrt/miniconda2/bin:$PATH"

# Add Bugzilla API key
export BUGZILLA_API_KEY="/home/harterrt/Private/.bugzilla_api"

# go stuff
export GOPATH=$HOME/go
export PATH=$PATH:$GOROOT/bin:$GOPATH/bin

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/harterrt/bin/google-cloud-sdk/path.bash.inc' ]; then source '/home/harterrt/bin/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/harterrt/bin/google-cloud-sdk/completion.bash.inc' ]; then source '/home/harterrt/bin/google-cloud-sdk/completion.bash.inc'; fi

# alias bfb="aws --profile bfb"

# Ruby stuff
export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"
export PATH="$HOME/.rbenv/plugins/ruby-build/bin:$PATH"

